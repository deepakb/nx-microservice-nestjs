generator client {
    provider = "prisma-client-js"
    output = "../../../node_modules/@prisma-clients/server"
}

datasource db {
    provider = "postgresql"
    url = env("AUTH_DATABASE_URL")
}

model User {
    id Int @id @default(autoincrement())
    name String
    email String @unique
    password String
    createdAt DateTime @default(now())
    updatedAt DateTime @updatedAt
}